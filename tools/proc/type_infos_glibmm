include type_infos_glibmm_generated

GObject <=> Glib::Object ## REFFED
gint <=> int ## EQUAL
gboolean <=> bool ## EQUAL
gchar <!> Glib::ustring ## USTRING
gchar <!> std::string ## STDSTRING
gchar <!> char ## EQUAL
gdouble <=> double ## EQUAL
GTimeSpan <=> Glib::TimeSpan ## EQUAL
GTimeVal <=> Glib::TimeVal ## EQUAL
GOptionContext <=> Glib::OptionContext ## NORMAL
GOptionGroup <=> Glib::OptionGroup ## NORMAL
GTimeZone <=> Glib::TimeZone ## NORMAL

# detailed

gchar** => Glib::StringArrayHandle:
{
  none: Glib::StringArrayHandle(##ARG##, ::Glib::OWNERSHIP_NONE)
  container: Glib::StringArrayhandle(##ARG##, ::Glib::OWNERSHIP_SHALLOW)
  full: Glib::StringArrayHandle(##ARG##, ::Glib::OWNERSHIP_DEEP)
}
